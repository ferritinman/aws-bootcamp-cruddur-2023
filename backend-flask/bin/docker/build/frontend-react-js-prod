#!/usr/bin/bash

docker build \
--build-arg REACT_APP_BACKEND_URL="https://api.ferritinman.com" \
--build-arg REACT_APP_AWS_PROJECT_REGION="$AWS_DEFAULT_REGION" \
--build-arg REACT_APP_AWS_COGNITO_REGION="$AWS_DEFAULT_REGION" \
--build-arg REACT_APP_AWS_USER_POOLS_ID="ca-central-1_czgFEqfTX" \
--build-arg REACT_APP_CLIENT_ID="17vmh2l62n9k69guut80jtnqsi" \
-t frontend-react-js \
-f Dockerfile.prod \
.
